

<scroll-view scroll-y class="scrollPage">
  <view class="UCenter-bg">
    <image src="https://api.xr1228.com/static/img/logo.png" class="png" mode="widthFix"></image>
    <view class="text-xl text-black text-bold">国家税务总局苏里格经济技术开发区税务局
    </view>
    <view class="margin-top-sm">
      <text class='text-white text-bold'>收入预测系统 ⏲️</text>
    </view>
    <image src="https://api.xr1228.com/static/img/wave.gif" mode="scaleToFill" class="gif-wave"></image>
  </view>
  <view class="padding flex text-center text-grey bg-white shadow-warp">
    <view class="flex flex-sub flex-direction solid-right">
      <view class="text-xxl text-orange">1.2K</view>
      <view class="margin-top-sm">
        <text class="cuIcon-attentionfill"></text> View</view>
    </view>
    <view class="flex flex-sub flex-direction solid-right">
      <view class="text-xxl text-blue">2651</view>
      <view class="margin-top-sm">
        <text class="cuIcon-favorfill"></text> Star</view>
    </view>
    <view class="flex flex-sub flex-direction">
      <view class="text-xxl text-green">38</view>
      <view class="margin-top-sm">
        <text class="cuIcon-peoplefill"></text> User</view>
    </view>
  </view>
	<!-- 登录窗体 -->
  <view class="cu-list menu card-menu margin-top-xl margin-bottom-xl shadow-lg radius">
    <view class="cu-bar justify-center bg-white">
      <view class="action sub-title">
        <text class="text-xl text-bold text-green">管理员登录</text>
        <text class="bg-green" style="width:2rem"></text>
      </view>
    </view>
    <form bindsubmit='formBindSubmit'>
      <view class="cu-form-group">
        <view class="title">名称</view>
        <input name="userID" value='' placeholder="请输入您的名字"></input>
      </view>
      <view class="cu-form-group">
        <view class="title">密码</view>
        <input name="password" value='' type='password' placeholder="请输入您的密码"></input>
      </view>
      <view class="padding flex flex-direction bg-white">
        <button class="cu-btn bg-blue lg" formType="submit">提交</button>
      </view>
    </form>
  </view>
  <view class="padding flex flex-direction bg-white">
        <button class="cu-btn bg-blue lg" bindtap='notManager'>非管理员用户</button>
      </view>

</scroll-view>